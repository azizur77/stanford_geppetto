{
  "name": "stanford/jumpstart",
  "description": "Jumpstart",
  "type": "project",
  "license": "GPL-2.0+",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.drupal-composer.org"
    }
  ],
  "require": {
    "composer/installers": "^1.0.21",
    "drupal/core": "~8.0",
    "wikimedia/composer-merge-plugin": "^1.3.0",
    "drupal/console": "dev-master",
    "drupal/webprofiler": "8.*",
    "drupal/coffee": "8.*",
    "drupal/composer_manager": "8.*",
    "drupal/devel": "8.*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ]
    }
  },
  "scripts": {
    "post-install-cmd": "scripts/composer/post-install.sh",
    "post-update-cmd": "scripts/composer/post-update.sh"
  }
}
